{
  "productName": "tools",
  "version": "0.0.0",
  "identifier": "tk.tools",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "tools",
        "width": 800,
        "height": 600,
        "decorations": false,
        "visible": false
      }
    ],
    "security": {
      "csp": "default-src 'self' customprotocol: asset:; img-src 'self' asset: https://asset.localhost",

      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": ["$TEMPLATE/**", "$RESOURCE/**", "$PICTURE"],
          "deny": ["$TEMPLATE/*.stronghold"]
        }
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
